<Window x:Class="Ruler.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ruler"
        mc:Ignorable="d"        
        Title="MainWindow" Height="450" Width="800">
    <Canvas>
        
        <local:CustomControl Canvas.Left="150" Canvas.Top="50" MajorCount="11" MinorCount="5" UnitCMSize="30" IsHorizontal="True" RotationAngle="30" />
        
        <local:CustomControl Canvas.Left="100" Canvas.Top="50" MajorCount="10" MinorCount="5" UnitCMSize="30" IsHorizontal="False"  />
    </Canvas>
</Window>
